{
  "extends": ["next/core-web-vitals", "@next/eslint-config-next"],
  "rules": {
    "@next/next/no-img-element": "off",
    "prefer-const": "error",
    "no-var": "error"
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
